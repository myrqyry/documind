{
  "name": "documind",
  "version": "1.0.0",
  "description": "Intelligent Document Scraper with Gemini API Integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --tsconfig src/tsconfig.json src/server.ts",
    "build": "tsc && vite build",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js,json}"
  },
  "keywords": [
    "document-processing",
    "ai",
    "gemini",
    "scraping",
    "context-generation",
    "mcp",
    "rag"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.27.0",
    "@modelcontextprotocol/sdk": "^1.0.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crawlee": "^3.7.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsdom": "^23.0.1",
    "mammoth": "^1.6.0",
    "marked": "^11.1.1",
    "mime-types": "^2.1.35",
    "mongodb": "^6.3.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.40.1",
    "rate-limiter-flexible": "^8.1.0",
    "redis": "^4.6.12",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/documind.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/documind/issues"
  },
  "homepage": "https://github.com/yourusername/documind#readme"
}
